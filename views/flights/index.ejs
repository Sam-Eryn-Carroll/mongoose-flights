<%- include('../partials/header') %>
  <table id="list">
        <thead>
            <tr>
                <th>Airline</th>
                <th>Airport</th>
                <th>Flight #</th>
                <th>Departure</th>
            </tr>
        </thead>
        <tbody>
      <!-- Write the line of EJS to iterate over movies using forEach -->
            <% flights.forEach(function(f) { %>
            <tr>
                <td><%= f.airline %></td>
                <td><%= f.airport %></td>
                <td><%= f.flightNo %></td>
                <td><%= f.departs %></td>
                <td><a href="flights/<%= f._id %>">Flight Details</a></td>
            </tr>
      <!-- Close the forEach using EJS here -->
            <% }); %>
        </tbody>
  </table>
  <%- include('../partials/footer') %>